{
  "name": "trip-check",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "frontend",
    "backend"
  ],
  "description": "**TripCheck** は、旅行者が自分で旅程を作成し、ルートや費用を自動でチェックできるモバイルアプリです。旅行計画を直感的に作成し、複数の経路・交通費・宿泊費などを比較・保存できます。",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "type:build": "npm run build --workspace=@trip-check/types",
    "util:build": "npm run build --workspace=@trip-check/utils",
    "start": "npm run start --workspace=frontend",
    "android": "npm run android --workspace=frontend",
    "ios": "npm run ios --workspace=frontend",
    "web": "npm run web --workspace=frontend",
    "lint": "npm run lint --workspace=frontend && npm run lint --workspace=backend",
    "lint:fix": "npm run lint:fix --workspace=backend",
    "build": "npm run build --workspace=backend",
    "deploy": "node -r dotenv/config ./node_modules/.bin/serverless deploy --verbose",
    "destroy": "npm run destroy --workspace=backend",
    "type-check": "npm run type-check --workspace=backend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kwisho/TripCheck.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kwisho/TripCheck/issues"
  },
  "homepage": "https://github.com/kwisho/TripCheck#readme",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.504.0",
    "@aws-sdk/client-sqs": "^3.536.0",
    "@aws-sdk/lib-dynamodb": "^3.504.0",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.9",
    "@react-navigation/native-stack": "^7.3.13",
    "@trip-check/types": "1.0.0",
    "@trip-check/utils": "1.0.0",
    "amazon-cognito-identity-js": "^6.3.15",
    "expo": "~53.0.9",
    "expo-blur": "~14.1.4",
    "expo-constants": "~17.1.6",
    "expo-font": "~13.3.1",
